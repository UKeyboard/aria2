# Basic Options
dir=你的下载目录
log=aria2c.log
input-file=aria2c.session
max-concurrent-downloads=5
continue=true
check-integrity=false

# Http/FTP/SFTP Options
connect-timeout=120
dry-run=false
lowest-speed-limit=8K
max-connection-per-server=10
max-file-not-found=3
max-tries=5
min-split-size=1M
netrc-path=.netrc
no-netrc=false
retry-wait=0
split=5

# Http Options
enable-http-keep-alive=true
check-certificate=false
http-no-cache=true

# FTP/SFTP Options
ftp-type=binary
ftp-reuse-connection=true

# BitTorrent/Metalink Options
bt-enable-lpd=true
bt-max-open-files=100
bt-max-peers=60
bt-metadata-only=false
bt-remove-unselected-file=true
bt-request-peer-speed-limit=100K
bt-save-metadata=true
follow-torrent=true
enable-dht6=true
bt-seed-unverified=true
bt-stop-timeout=30
dht-file-path=dnt.dat
dht-file-path6=dnt6.dat
dht-listen-port=6881-6999
enable-dht=true
enable-dht6=true
listen-port=6881-6999
max-overall-upload-limit=20K
max-upload-limit=8K
seed-ratio=1.2
seed-time=30

# RPC Options
enable-rpc=true
pause=false
pause-metadata=false
rpc-allow-origin-all=true
rpc-listen-all=true
rpc-listen-port=6800
rpc-save-upload-metadata=true
#rpc-secret=aria2rpc
rpc-secure=false

# Advanced Options
enable-async-dns6=true
allow-overwrite=true
auto-file-renaming=false
always-resume=true
async-dns=true
auto-save-interval=60
conditional-get=true
console-log-level=notice
daemon=true
disable-ipv6=false
disk-cache=64M
enable-color=true
file-allocation=falloc 
force-save=false
save-not-found=false
human-readable=true
keep-unfinished-download-result=true
max-resume-failure-tries=3
force-sequential=false
max-download-limit=3M
parameterized-uri=true
save-session=aria2c.session
save-session-interval=60
log-level=error
max-overall-download-limit=0
no-file-allocation-limit=32M
